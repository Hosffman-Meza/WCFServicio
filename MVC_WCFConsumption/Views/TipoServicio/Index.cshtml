@model IEnumerable<MVC_WCFConsumption.ServiceReferenceServicios.SP_Select_TipoServicio_Result>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear nuevo Tipo de servicio", "insertTipoServicio", "TipoServicio",null)
</p>
<table class="table">
    <tr>
        <th>
            Id Tipo de Servicio
        </th>
        <th>
            Descripción
        </th>
        <th>
            Estado
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>

            @Html.DisplayFor(modelItem => item.IdTipoServicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "updateTipoServicio", "TipoServicio", new { idTS = @item.IdTipoServicio }, null) |
            @Html.ActionLink("Eliminar", "deleteTipoServicio", "TipoServicio", new { idTS = item.IdTipoServicio }, null)
        </td>
    </tr>
}

</table>
